{
  "environment": "development",
  "debug": true,
  "log_level": "DEBUG",
  "services": {
    "api": {
      "host": "localhost",
      "port": 8000,
      "workers": 2,
      "timeout": 60
    },
    "database": {
      "host": "localhost",
      "port": 5432,
      "name": "geoai_dev",
      "user": "dev_user",
      "connection_pool": 5,
      "timeout": 30
    },
    "cache": {
      "host": "localhost",
      "port": 6379,
      "ttl": 300
    },
    "storage": {
      "type": "local",
      "path": "./data/storage",
      "max_size_mb": 1024
    }
  },
  "features": {
    "enable_experimental": true,
    "enable_profiling": true,
    "enable_debug_endpoints": true
  },
  "model": {
    "path": "./models/development",
    "precision": "mixed",
    "batch_size": 4,
    "cache_predictions": true
  },
  "monitoring": {
    "metrics_enabled": true,
    "tracing_enabled": true,
    "log_requests": true,
    "performance_tracking": true
  },
  "cors": {
    "origins": ["*"],
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allow_credentials": true
  }
}
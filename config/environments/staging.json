{
  "environment": "staging",
  "debug": false,
  "log_level": "INFO",
  "services": {
    "api": {
      "host": "0.0.0.0",
      "port": 8000,
      "workers": 4,
      "timeout": 60
    },
    "database": {
      "host": "geoai-staging-db",
      "port": 5432,
      "name": "geoai_staging",
      "user": "staging_user",
      "connection_pool": 10,
      "timeout": 30
    },
    "cache": {
      "host": "geoai-staging-redis",
      "port": 6379,
      "ttl": 600
    },
    "storage": {
      "type": "s3",
      "bucket": "geoai-staging-storage",
      "region": "us-east-1",
      "max_size_mb": 5120
    }
  },
  "features": {
    "enable_experimental": true,
    "enable_profiling": true,
    "enable_debug_endpoints": false
  },
  "model": {
    "path": "/opt/models/staging",
    "precision": "mixed",
    "batch_size": 8,
    "cache_predictions": true
  },
  "monitoring": {
    "metrics_enabled": true,
    "tracing_enabled": true,
    "log_requests": true,
    "performance_tracking": true
  },
  "cors": {
    "origins": ["https://staging-app.geoai-research.org", "http://localhost:3000"],
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allow_credentials": true
  }
}